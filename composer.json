{
    "name": "lavendar77/slarb",
    "description": "A Simple API Response Builder for Laravel Apps",
    "type": "project",
    "homepage": "https://github.com/lavendar77/slarb",
    "keywords": ["api", "laravel", "response builder"],
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Adeyinka M. Adefolurin",
            "email": "folurinyinka@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Lavendar77\\Slarb\\": "src"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Lavendar77\\Slarb\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lavendar77\\Slarb\\SlarbServiceProvider"
            ],
            "aliases": {
                "Slarb": "Lavendar77\\Slarb\\Slarb"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
